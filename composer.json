{
    "name": "quintanilhar/assessment-api",
    "description": "The easist way to manage multiple-choice questions in your assessments.",
    "type": "project",
    "authors": [
        {
            "name": "Ricardo Quintanilha",
            "email": "ricardo.aquintanilha@gmail.com"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "require": {
        "php-di/php-di": "^6.3",
        "psr/container": "^1.0",
        "ramsey/uuid": "^4.1",
        "slim/psr7": "^1.4",
        "slim/slim": "^4.8",
        "stichoza/google-translate-php": "^4.1",
        "symfony/serializer": "^5.3",
        "vlucas/phpdotenv": "^5.3",
        "webmozart/assert": "^1.10"
    },
    "autoload": {
        "psr-4": {
            "Quintanilhar\\AssessmentApi\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Quintanilhar\\AssessmentApi\\Test\\": "test/"
        }
    },
    "config": {
        "process-timeout": 0,
        "sort-packages": true
    }
}
